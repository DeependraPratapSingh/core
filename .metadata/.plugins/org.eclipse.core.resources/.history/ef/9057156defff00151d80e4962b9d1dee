package MapCollection;

import java.util.Hashtable;

/**
 * @author DnZdP
 *
 */
public class MapDemo1 {
	public static void main(String[] args) {
		Hashtable<String, Integer> h1 = new Hashtable<String, Integer>();
		h1.put("dp01", 001);
		h1.put("dp02", 002);
		h1.put("dp03", 003);
		h1.put("dp04", 004);
	}
}
